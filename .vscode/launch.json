  "version": "0.2.0",
  "configurations": [
    {
      "name": "daily_mood",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["--dart-define=URL='${input:MongoURL}'"]
    },
    {
      "name": "daily_mood (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define=URL=${input:MongoURL}"]
    }
  ],
  "inputs": [
    {
      "id": "MongoURL",
      "type": "promptString",
      "description": "Enter MongoDB URL"
    }
  ]
}
